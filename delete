      products+='\r\n';
      if( document.getElementById('zap-ofer-olej-cbd-mct-30-10g').checked ) products += '\r\n' + productname + ' --- ' + '10g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-30-30g').checked ) products += '\r\n' + productname + ' --- ' + '30g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-30-250g').checked ) products += '\r\n' + productname + ' --- ' + '250g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-30-1000g').checked ) products += '\r\n' + productname + ' --- ' + '1000g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-30-5000g').checked ) products += '\r\n' + productname + ' --- ' + '5000g';

      products += '\r\n\r\n';
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');
      products = products.replace('\r\n\r\n\r\n','\r\n\r\n');

      console.log(products);

      previouslyEnteredText = document.getElementById('form-message').value;

      let inquiry = "";
      if( previouslyEnteredText != '' )
        inquiry = previouslyEnteredText + "\r\n\r\n-----------------------------------\r\n\r\n" + header + products + footer;
      else
        inquiry = header + products + footer;

      document.getElementById('form-message').innerHTML = inquiry;
      document.getElementById('form-message').value = inquiry;
      
      document.getElementById("name").focus();
    }

    function buildCooperationInquiry() {
      console.log('buildCooperationInquiry FUNCTION CALLED');
      var header="Omówmy możliwość współpracy.";
      var innerText="\r\n\r\n<TUTAJ OPISZ SWOJĄ WIZJĘ>\r\n\r\n"; 
      var footer="Oświadczam, że zapoznałem się i akceptuję zasady określone w dokumencie Polityka Prywatności firmy Canabical.\r\n\r\nDziękuję.";

      previouslyEnteredText = document.getElementById('form-message').value;

      let inquiry = "";
      if( previouslyEnteredText != '' )
        inquiry = previouslyEnteredText + "\r\n\r\n-----------------------------------\r\n\r\n" + header + innerText + footer;
      else
        inquiry = header + innerText + footer;

      document.getElementById('form-message').innerHTML = inquiry;
      document.getElementById('form-message').value = inquiry;
      
      document.getElementById("name").focus();
    }
  </script>



