      if( document.getElementById('zap-ofer-olej-cbd-mct-10-5000g').checked ) products += '\r\n' + productname + ' --- ' + '5000g';

      productname = 'Olej CBD MCT Full Spectrum 20%';
      products+='\r\n';
      if( document.getElementById('zap-ofer-olej-cbd-mct-20-10g').checked ) products += '\r\n' + productname + ' --- ' + '10g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-20-30g').checked ) products += '\r\n' + productname + ' --- ' + '30g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-20-250g').checked ) products += '\r\n' + productname + ' --- ' + '250g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-20-1000g').checked ) products += '\r\n' + productname + ' --- ' + '1000g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-20-5000g').checked ) products += '\r\n' + productname + ' --- ' + '5000g';

