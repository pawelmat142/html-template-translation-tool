  <script type="text/javascript">
    // this self executing function is triggered in every browser imediatelly after DOM is loaded
    (function() {
      // Just after loading DOM get section id from URL (if specified) and make the proper
      //  menu option active.
      var baseUrl = (window.location).href; // You can also use document.URL
      var sectionId = baseUrl.substring(baseUrl.lastIndexOf('#') + 1);
      //alert(sectionId);

      document.getElementById('nav-item-1').classList.remove('selected-nav-item');
      
      switch (sectionId) {
        case 'o-nas':
          document.getElementById('nav-item-1').className+=' selected-nav-item';   
          break;
        case 'produkty':
          document.getElementById('nav-item-2').className+=' selected-nav-item';   
          break;
        case 'ekstrakcja-uslugowa':
          document.getElementById('nav-item-3').className+=' selected-nav-item';   
          break;
        case 'stworz-wlasna-marke':
          document.getElementById('nav-item-4').className+=' selected-nav-item';   
          break;
        case 'oferta':
          document.getElementById('nav-item-5').className+=' selected-nav-item';   
          break;
        case 'kontakt':
          document.getElementById('nav-item-6').className+=' selected-nav-item';   
          break;
        default:
          document.getElementById('nav-item-1').className+=' selected-nav-item';   
      }

  // document.getElementById('nav-item-2').classList.remove('selected-nav-item'); 

  // document.getElementById('nav-item-5').classList.remove('selected-nav-item'); 
  // document.getElementById('nav-item-6').classList.remove('selected-nav-item'); 
  // document.getElementById('nav-item-2').className+=' selected-nav-item'; 

      // var request = new XMLHttpRequest()
      // request.open('GET', 'map-info-window-text.txt', false)
      // request.send()
      // let map_info_window_text = request.responseText        

      // document.getElementById( 'global-variables' ).setAttribute( 'map-info-window-text', map_info_window_text );
    })();   
  </script>   





  <script type="text/javascript">
    function buildProductsInquiry() {
      console.log('buildProductsInquiry FUNCTION CALLED');
      var header="Proszę o ofertę cenową następujących produktów:";
      var products=""; 
      var footer="Oświadczam, że zapoznałem się i akceptuję zasady określone w dokumencie Polityka Prywatności firmy Canabical.\r\n\r\nDziękuję.";

      var productname = '';

      productname = 'Cannabis Sativa Leaf Extract 10% CBD';
      products+='\r\n';
      if( document.getElementById('zap-ofer-leaf-extract-10-10g').checked ) products += '\r\n' + productname + ' --- ' + '10g';
      if( document.getElementById('zap-ofer-leaf-extract-10-30g').checked ) products += '\r\n' + productname + ' --- ' + '30g';
      if( document.getElementById('zap-ofer-leaf-extract-10-250g').checked ) products += '\r\n' + productname + ' --- ' + '250g';
      if( document.getElementById('zap-ofer-leaf-extract-10-1000g').checked ) products += '\r\n' + productname + ' --- ' + '1000g';
      if( document.getElementById('zap-ofer-leaf-extract-10-5000g').checked ) products += '\r\n' + productname + ' --- ' + '5000g';

      productname = 'Cannabis Sativa Leaf Extract  1% CBD THC free';
      products+='\r\n';
      if( document.getElementById('zap-ofer-leaf-extract-01-1kg').checked ) products += '\r\n' + productname + ' - ' + '1kg';
      if( document.getElementById('zap-ofer-leaf-extract-01-5kg').checked ) products += '\r\n' + productname + ' - ' + '5kg';
      if( document.getElementById('zap-ofer-leaf-extract-01-10kg').checked ) products += '\r\n' + productname + ' - ' + '10kg';

      productname = 'Olej CBD MCT Full Spectrum  5%';
      products+='\r\n';
      if( document.getElementById('zap-ofer-olej-cbd-mct-5-10g').checked ) products += '\r\n' + productname + ' --- ' + '10g';
      if( document.getElementById('zap-ofer-olej-cbd-mct-5-30g').checked ) products += '\r\n' + productname + ' --- ' + '30g';
